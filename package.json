{
	"name": "jblund-dealers",
	"version": "1.0.0",
	"description": "JBLund Dealers WordPress Plugin - Frontend Assets",
	"scripts": {
		"watch": "sass --watch assets/scss/dealers.scss:assets/css/dealers.css assets/scss/dashboard.scss:modules/dealer-portal/assets/css/dashboard.css --style expanded --source-map",
		"build": "sass assets/scss/dealers.scss:assets/css/dealers.css assets/scss/dashboard.scss:modules/dealer-portal/assets/css/dashboard.css --style compressed --no-source-map",
		"dev": "sass assets/scss/dealers.scss:assets/css/dealers.css assets/scss/dashboard.scss:modules/dealer-portal/assets/css/dashboard.css --style expanded --source-map"
	},
	"devDependencies": {
		"sass": "^1.69.0"
	},
	"author": "MacphersonDesigns",
	"license": "GPL-2.0+"
}
